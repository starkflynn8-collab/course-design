          <el-row style=\"margin: 10px\">\n              <el-col v-for=\"item in goods\" :key=\"item.id\" :span=\"8\">\n                <el-card :header=\"item.gname\" v-if=\"item.gcount > 0\">\n                  <el-image :src=\"getImageUrl(item.gimage)\" fit=\"cover\" style=\"height: 200px;\"></el-image>\n                  <div style=\"padding: 14px\">\n                    <div class=\"bottom clearfix\">\n                      <span\n                        style=\"\n                          display: inline-block;\n                          text-align: left;\n                          width: 100%;\n                        \"\n                        ><b>描述：</b> {{ item.gcontent }} </span\n                      ><br />\n                      <span\n                        style=\"\n                          display: inline-block;\n                          text-align: left;\n                          width: 100%;\n                        \"\n                        ><b>价格：</b>{{ item.gprice }} 元</span\n                      >\n                      <span\n                        style=\"\n                          display: inline-block;\n                          text-align: left;\n                          width: 100%;\n                        \"\n                        ><b>数量：</b>{{ item.gcount }} 个</span\n                      ><br />\n                      <div style=\"display: flex; align-items: center; margin-top: 4px;\">\n                        <span style=\"margin-right: 6px;\"><b>新旧程度：</b></span>\n                        <el-rate\n                            v-model=\"item.glevel\"\n                            disabled\n                            text-color=\"#ff9900\"\n                        />\n                      </div>\n\n                      <span\n                          style=\"\n    display: inline-block;\n    text-align: left;\n    width: 100%;\n  \"\n                          v-if=\"item.gcontact\"\n                      >\n  <b>联系方式：</b>{{ item.gcontact }}\n</span>\n\n                      <el-button type=\"text\" class=\"button\" @click=\"onBuy(item)\"\n                        >购买</el-button\n                      >\n                    </div>\n                  </div>\n                </el-card>\n              </el-col>\n            </el-row>\n          </el-tab-pane>\n        </el-tabs>\n      </el-row>\n    </div>\n    <el-pagination\n      @size-change=\"handleSizeChange\"\n      @current-change=\"handleCurrentChange\"\n      :current-page=\"currentPage\"\n      :page-sizes=\"pageLimits\"\n      :page-size=\"pageSize\"\n      layout=\"total, sizes, prev, pager, next, jumper\"\n      :total=\"itemTotal\"\n    >\n    </el-pagination>\n  </div>\n</template>\n\n<script>\nimport lunbo1 from '@/assets/images/lunbo1.png';\nimport lunbo2 from '@/assets/images/lunbo2.png';\nexport default {\n  name: \"Index\",\n  data() {\n\n    return {\n      activeIndex: \"1\",\n      carouselGoods: [\n        { gid: 1, gimage: lunbo1 },\n        { gid: 2, gimage: lunbo2 },\n      ],\n      activeName: \"all\",\n      goods: [],\n      labels: [\"全部\", \"通识必修\", \"专业核心\", \"考研资料\", \"校区专区\"],\n      names: [\"all\", \"common\", \"major\", \"post\", \"campus\"],\n      currentPage: 1,\n      pageLimits: [10, 20, 30, 40, 50],\n      pageSize: 10,\n      itemTotal: 10,\n      searchName: \"\",\n      loginFlag: false,\n      token: null,\n      currGoods: {},\n      currUser: null,\n      apiBase: 'http://localhost:8888',\n      homeFilter: {\n        subject: \"\",\n        type: \"\",\n        campus: \"\"\n      },\n\n    };\n  },\n  methods: {\n\n    getImageUrl(gimage) {\n      if (!gimage) return '/default-book.jpg';  // 没图显示默认\n      if (gimage.startsWith('http')) return gimage;  // 老外链直接用\n      return this.apiBase + gimage;  // 本地上传拼接端口\n    },\n\n    toLogin() {\n      this.$router.push({ path: \"/Login\" });\n    },\n    toRegister() {\n      this.$router.push({ path: \"/Register\" });\n    },\n    handleClick(tab) {\n      // 每次切换先清空\n      this.homeFilter = {\n        subject: \"\",\n        type: \"\",\n        campus: \"\"\n      };\n\n      switch (tab.name) {\n        case \"all\":\n          break;\n\n        case \"common\": // 通识必修\n          this.homeFilter.subject = \"通识基础课\";\n          break;\n\n        case \"major\": // 专业核心\n          this.homeFilter.subject = \"专业课\";\n          break;\n\n        case \"post\": // 考研\n          this.homeFilter.type = \"考研\";\n          break;\n\n        case \"campus\": // 校区专区（先默认九龙湖）\n          this.homeFilter.campus = \"九龙湖\";\n          break;\n      }\n\n      this.currentPage = 1;\n      this.getGoods(this.currentPage, this.pageSize);\n    },\n\n    getGoods(page, limit) {\n      this.axios\n        .get(\n          \"http://localhost:8888/goods/search\",\n          {\n            params: {\n              gName: this.searchName,\n              page: page,\n              limit: limit,\n            },\n          },\n          { emulateJSON: true }\n        )\n        .then((resp) => {\n          if (resp.data.message === \"ok\") {\n            this.itemTotal = resp.data.data.total;\n            this.goods = resp.data.data.records;\n          }\n        })\n        .catch((error) => {\n          console.log(error);\n        });\n    },\n    handleSizeChange(limit) {\n      this.limit = limit;\n      this.currentPage = 1;\n      this.getGoods(this.currentPage, this.limit);\n    },\n    handleCurrentChange(val) {\n      this.currentPage = val;\n      this.getGoods(this.currentPage, this.limit);\n    },\n    toCarousel() {},\n    onBuy(res) {\n      console.log(res);\n      if (!this.token) {\n        this.$message({\n          message: \"请先登录\",\n          type: \"error\",\n        });\n      } else {\n        this.currGoods = res;\n\n        this.$confirm(\"确认购买后将生成订单信息\", \"确认信息\", {\n          distinguishCancelAndClose: true,\n          confirmButtonText: \"确认购买\",\n          cancelButtonText: \"取消\",\n        })\n          .then(() => {\n            this.buyGoods();\n          })\n          .catch((action) => {\n            this.$message({\n              type: \"warning\",\n              message: \"已放弃购买\",\n            });\n          });\n      }\n    },\n    toLoginout() {\n      console.log(this.token, \"<<<<<token\");\n      this.axios\n        .post(\"http://localhost:8888/user/loginout\", null, {\n          params: {\n            token: this.token,\n          },\n        })\n        .then((resp) => {\n          console.log(resp);\n          localStorage.removeItem(\"token\");\n          this.loginFlag = false;\n          this.token = null;\n          this.$message({\n            message: \"退出成功\",\n            type: \"success\",\n          });\n        })\n        .catch((error) => {\n          console.log(error);\n        });\n    },\n    toHome() {\n      this.$router.push({ path: \"/Home\" });\n    },\n    buyGoods() {\n      this.axios.defaults.headers.post[\"Content-Type\"] =\n        \"application/x-www-form-urlencoded\";\n      const params = new URLSearchParams();\n      params.append(\"token\", this.token);\n      params.append(\"dAmount\", this.currGoods.gprice);\n      params.append(\"dCount\", 1);\n      params.append(\"sId\", this.currGoods.uid);\n      params.append(\"gId\", this.currGoods.gid);\n      params.append(\"bId\", this.currUser.uid);\n\n      this.axios\n        .post(\"http://localhost:8888/order/add\", params)\n        .then((resp) => {\n          console.log(resp);\n          this.getGoods();\n          this.$message({\n            type: \"success\",\n            message: \"订单生成成功，请前往个人中心完成支付  \",\n          });\n          this.currGoods = {};\n        })\n        .catch((error) => {\n          console.log(error);\n        });\n      this.currGoods = {};\n    },\n  },\n\n  created() {\n    this.getGoods(1, 10);\n    let token = localStorage.getItem(\"token\");\n    console.log(token);\n    if (token === null) {\n      this.loginFlag = false;\n      this.token = null;\n      this.currUser = null;\n    } else {\n      this.token = token;\n      this.loginFlag = true;\n      this.currUser = JSON.parse(localStorage.getItem(\"currUser\"));\n    }\n  },\n};\n</script>\n\n<style>\n.block {\n  margin: 5px 80px;\n}\n\n.el-carousel__item h3 {\n  color: #475669;\n  font-size: 14px;\n  opacity: 0.75;\n  line-height: 150px;\n  margin: 0;\n}\n\n.el-carousel__item:nth-child(2n) {\n  background-color: #99a9bf;\n}\n\n.el-carousel__item:nth-child(2n + 1) {\n  background-color: #d3dce6;\n\n}\n\n.tab {\n  margin: 50px;\n}\n</style>"],"mappings":";;AA4HA,OAAAA,MAAA;AACA,OAAAC,MAAA;AACA;EACAC,IAAA;EACAC,KAAA;IAEA;MACAC,WAAA;MACAC,aAAA,GACA;QAAAC,GAAA;QAAAC,MAAA,EAAAP;MAAA,GACA;QAAAM,GAAA;QAAAC,MAAA,EAAAN;MAAA,EACA;MACAO,UAAA;MACAC,KAAA;MACAC,MAAA;MACAC,KAAA;MACAC,WAAA;MACAC,UAAA;MACAC,QAAA;MACAC,SAAA;MACAC,UAAA;MACAC,SAAA;MACAC,KAAA;MACAC,SAAA;MACAC,QAAA;MACAC,OAAA;MACAC,UAAA;QACAC,OAAA;QACAC,IAAA;QACAC,MAAA;MACA;IAEA;EACA;EACAC,OAAA;IAEAC,YAAApB,MAAA;MACA,KAAAA,MAAA;MACA,IAAAA,MAAA,CAAAqB,UAAA,iBAAArB,MAAA;MACA,YAAAc,OAAA,GAAAd,MAAA;IACA;;IAEAsB,QAAA;MACA,KAAAC,OAAA,CAAAC,IAAA;QAAAC,IAAA;MAAA;IACA;IACAC,WAAA;MACA,KAAAH,OAAA,CAAAC,IAAA;QAAAC,IAAA;MAAA;IACA;IACAE,YAAAC,GAAA;MACA;MACA,KAAAb,UAAA;QACAC,OAAA;QACAC,IAAA;QACAC,MAAA;MACA;MAEA,QAAAU,GAAA,CAAAjC,IAAA;QACA;UACA;QAEA;UAAA;UACA,KAAAoB,UAAA,CAAAC,OAAA;UACA;QAEA;UAAA;UACA,KAAAD,UAAA,CAAAC,OAAA;UACA;QAEA;UAAA;UACA,KAAAD,UAAA,CAAAE,IAAA;UACA;QAEA;UAAA;UACA,KAAAF,UAAA,CAAAG,MAAA;UACA;MACA;MAEA,KAAAb,WAAA;MACA,KAAAwB,QAAA,MAAAxB,WAAA,OAAAE,QAAA;IACA;IAEAsB,SAAAC,IAAA,EAAAC,KAAA;MACA,KAAAC,KAAA,CACAC,GAAA,CACA,sCACA;QACAC,MAAA;UACAC,KAAA,OAAA1B,UAAA;UACAqB,IAAA,EAAAA,IAAA;UACAC,KAAA,EAAAA;QACA;MACA,GACA;QAAAK,WAAA;MAAA,CACA,EACAC,IAAA,CAAAC,IAAA;QACA,IAAAA,IAAA,CAAA1C,IAAA,CAAA2C,OAAA;UACA,KAAA/B,SAAA,GAAA8B,IAAA,CAAA1C,IAAA,CAAAA,IAAA,CAAA4C,KAAA;UACA,KAAAtC,KAAA,GAAAoC,IAAA,CAAA1C,IAAA,CAAAA,IAAA,CAAA6C,OAAA;QACA;MACA,GACAC,KAAA,CAAAC,KAAA;QACAC,OAAA,CAAAC,GAAA,CAAAF,KAAA